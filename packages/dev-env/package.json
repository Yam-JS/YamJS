{
  "name": "@yam-js/dev-env",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "tsnode": "tsx --tsconfig tsconfig-node.json ",
    "lint": "yarn lint:src & yarn lint:scripts",
    "lint:src": "eslint ./src --max-warnings=0",
    "lint:scripts": "eslint ./scripts --max-warnings=0",
    "prettier": "prettier --check ./src/**/*.{ts,tsx} ./scripts/**/*.{ts,tsx}",
    "setup:yam": "yarn tsnode ./scripts/setupYam",
    "setup:paper": "yarn tsnode ./scripts/setupPaper",
    "setup:plugman": "yarn tsnode ./scripts/setupPlugman",
    "setup:sync": "yarn tsnode ./scripts/start",
    "start": "yarn setup:yam & yarn setup:sync & concurrently -c \"auto\" \"yarn:start:*\"",
    "start:yam": "yarn tsnode ./scripts/startYam",
    "start:js": "yarn yamjs-dev-webpack",
    "start:server": "cd server & java -Xms2G -Xmx2G -jar -agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=5005 server.jar -nogui",
    "typecheck": "tsc --noEmit",
    "yamjs": "yarn --cwd ../../"
  },
  "dependencies": {
    "@yam-js/core": "^0.1.4",
    "@yam-js/legacy": "^0.1.4",
    "@yam-js/dev-reload": "^0.1.5",
    "vlq": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@graal-types/java-16": "^1.0.0",
    "@graal-types/paper-1.19": "^1.0.1",
    "@types/node": "^20.4.4",
    "babel-loader": "^9.1.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-graal-imports": "^0.0.3",
    "chokidar": "^3.5.3",
    "concurrently": "^8.0.1",
    "tsx": "^3.12.5",
    "typescript": "^5.1.3",
    "webpack": "^5.76.1",
    "webpack-cli": "^4.7.0"
  }
}
