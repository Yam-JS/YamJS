{
  "name": "@yam-js/dev-env-paper-webpack",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "tsnode": "tsx --tsconfig tsconfig-node.json ",
    "lint": "yarn lint:src",
    "lint:src": "eslint ./src --max-warnings=0",
    "prettier": "prettier --check ./src/**/*.{ts,tsx}",
    "setup:yam": "yarn tsnode ../scripts/setupYam",
    "setup:paper": "yarn tsnode ../scripts/setupPaper",
    "setup:plugman": "yarn tsnode ../scripts/setupPlugman",
    "setup:sync": "yarn tsnode ../scripts/sync",
    "start": "yarn setup:yam & yarn setup:sync js & concurrently -c \"auto\" \"yarn:start:*\"",
    "start:yam": "yarn tsnode ../scripts/startYam",
    "start:server": "cd server & java -Xms2G -Xmx2G -jar -agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=5005 server.jar -nogui",
    "typecheck": "tsc --noEmit",
    "yamjs": "yarn --cwd ../../../"
  },
  "dependencies": {
    "@yam-js/core": "^0.1.3",
    "@yam-js/legacy": "^0.1.2",
    "@yam-js/dev-reload": "^0.1.3",
    "vlq": "^2.0.4"
  },
  "devDependencies": {
    "@graal-types/java-16": "^1.0.0",
    "@graal-types/paper-1.19": "^1.0.1",
    "@types/node": "^18.15.2",
    "chokidar": "^3.5.3",
    "concurrently": "^8.0.1",
    "tsx": "^3.12.5",
    "typescript": "^5.0.3"
  }
}
