{
  "name": "@yam-js/dev",
  "private": true,
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "tsnode": "tsx --tsconfig tsconfig-node.json ",
    "setup:yam": "yarn tsnode ./scripts/setupYam",
    "setup:paper": "yarn tsnode ./scripts/setupPaper",
    "setup:plugman": "yarn tsnode ./scripts/setupPlugman",
    "start": "yarn setup:yam & concurrently -c \"auto\" \"yarn:start:*\"",
    "start:yam": "yarn tsnode ./scripts/startYam",
    "start:js": "yarn yamjs-webpack",
    "start:server": "cd server & java -Xms2G -Xmx2G -jar -agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=5005 server.jar -nogui",
    "yamjs": "yarn --cwd ../../",
    "test": "yarn yamjs-dev-watcher src"
  },
  "dependencies": {
    "@yam-js/core": "0.0.1",
    "@yam-js/legacy": "0.0.1",
    "@yam-js/dev-reload": "0.0.1",
    "vlq": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@graal-types/java-16": "^1.0.0",
    "@graal-types/paper-1.19": "^1.0.1",
    "@types/node": "^18.15.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-graal-imports": "^0.0.3",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "tsx": "^3.12.5",
    "typescript": "^4.9.5",
    "webpack": "^5.76.1",
    "webpack-cli": "^4.7.0"
  }
}
