{
  "name": "yam-js",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "author": "Kyle <kyle.mercer.89@gmail.com>",
  "license": "MIT",
  "scripts": {
    "yamjs": "yarn",
    "build": "yarn workspace @yam-js/build",
    "core": "yarn workspace @yam-js/core",
    "dev": "yarn workspace @yam-js/dev-env",
    "e2e": "yarn workspace @yam-js/e2e",
    "graaltype": "yarn --cwd ./packages/graal-type-introspection",
    "legacy": "yarn workspace @yam-js/legacy",
    "plugin": "yarn workspace @yam-js/plugin",
    "testutil": "yarn workspace @yam-js/test-util",
    "typecheck:all": "yarn core typecheck && yarn dev typecheck && yarn e2e typecheck && yarn graaltype typecheck && yarn legacy typecheck && yarn testutil typecheck",
    "build:libs": "yarn core build && yarn legacy build && yarn testutil build",
    "changeset": "changeset",
    "version": "changeset version",
    "version:dev": "changeset version --snapshot dev",
    "release": "changeset publish",
    "release:dev": "changeset publish --tag dev"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "got": "^11.8.6",
    "tsx": "^3.12.5",
    "typescript": "^4.9.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@1.22.5"
}
